startUp: Window {
	alignment: Alignment center,

	app_logo: Image {
		resource: string "Images/Title.png"
	},
	select_button: Button {
		title: string "Select source script",
		pressed: void event %{
			var url = startUp.selectInputFile("Select Amber Script", "amb") ;
			if(url != null){
				console.log("URL =" + url.absoluteString + "\n")
				var vid = startUp.loadView(url)
				if(vid != null){
					console.log("vid =" + vid + "\n")
					startUp.selectView(vid)
				}
			}
		%}
	}
}
